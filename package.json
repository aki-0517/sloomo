{
  "name": "solana-mobile-sloomo",
  "version": "1.0.0",
  "description": "Solana Mobile App",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android-debug": "cd android && ./gradlew assembleDebug",
    "clean:android": "cd android && ./gradlew clean",
    "typecheck": "tsc --noEmit",
    "portfolio:init": "cd contract && yarn portfolio:init",
    "portfolio:deposit": "cd contract && yarn portfolio:deposit",
    "portfolio:add-allocation": "cd contract && yarn portfolio:add-allocation",
    "portfolio:check-usdc": "cd contract && yarn portfolio:check-usdc",
    "portfolio:check-sol": "cd contract && yarn portfolio:check-sol", 
    "portfolio:check": "cd contract && yarn portfolio:check",
    "portfolio:rebalance": "cd contract && yarn portfolio:rebalance"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@solana-mobile/mobile-wallet-adapter-protocol": "^2.0.0",
    "@solana-mobile/mobile-wallet-adapter-protocol-web3js": "^2.0.0",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "@supabase/supabase-js": "^2.38.5",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-paper": "^5.12.3",
    "react-native-vector-icons": "^10.0.3",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-camera": "^4.2.1",
    "react-native-get-random-values": "^1.9.0",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/react-native": "^12.4.2",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}